  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:backend": "cross-env NODE_ENV=production node dist/apps/nestjs-backend/main",
    "start:giftcard": "cross-env NODE_ENV=production node dist/apps/giftcard/main",
    "start:blockchain": "cross-env NODE_ENV=production node dist/apps/blockchain-app/main",
    "start:payment": "cross-env NODE_ENV=production node dist/apps/payment/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/nestjs-backend/test/jest-e2e.json"
  },
